{
  "rules": {
    "indent": [
      2,
      2,
      {
        "SwitchCase": 1
      }
    ],
    "semi": [
      2,
      "always"
    ],
    "quotes": [
      2,
      "single"
    ],
    "eqeqeq": 1,
    "strict": 0,
    "no-console": 0,
    "no-process-env": 0
  },
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module"
  },
  "env": {
    "node": true,
    "es6": true,
    "browser": true
  },
  "extends": "eslint:recommended",
  "globals": {
    "fullpage": true,
    "Swiper": true
  }
}
